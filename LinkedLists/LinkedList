class Node {
  constructor (data, next = null) {
    this.value = data;
    this.next = next;
  }
};

class SinglyLinkedList {
  constructor() {
    this.head = null;
    this.tail = null;
    this.length = 0;
  }

  getLength() {
    return this.size;
  };

  append(data) {
    const node = new Node(data);
    console.log(node);
    if (this.count == 0) {
      this.head = node.data;
    } else {
      this.tail.next = node;
    }
    
    this.tail = node;
    this.length++;
  };
  
}


